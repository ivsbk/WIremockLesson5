{
	"request": {
		"urlPathPattern": "/test", 
		"bodyPatterns" :  [
			{
      		"matchesJsonPath" : "$[?(@.telephone_number=~/\\+[0-9]{11}/)]"
    	}
    ], 
    	"method": "POST"
	},
	"response":{
		"status": 200,
		"jsonBody": {
			"telphone.number": "{{jsonPath request.body '$.telephone_number'}}"
		},
		"headers": {"Content-Type": "application/json"}
	}
}